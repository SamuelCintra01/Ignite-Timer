{
  "extends": "@rocketseat/eslint-config/react",
  "rules": {
    "no-unused-vars": "off",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": false,
        "arrowParens": "always",
        "semi": true,
        "endOfLine": "auto"
      }
    ],
    "@typescript-eslint/no-explicit-any": "off"
  }

}